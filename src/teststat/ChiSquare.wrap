.def("register_fit_components", &ChiSquare::RegisterFitComponents)
.def("get_parameter_count", &ChiSquare::GetParameterCount)
.def("set_parameters", &ChiSquare::SetParameters)
.def("get_parameters", &ChiSquare::GetParameters)
.def("get_parameter_names", &ChiSquare::GetParameterNames);
